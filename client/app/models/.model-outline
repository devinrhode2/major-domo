Residence
  hasMany:
    -users
    -chores
    -expenses

User
  belongsTo: residence
  hasMany:
    -tallies
    -purchases


Chore
  belongsTo:
    -residence
  hasMany:
    -tallies

Expense
  belongsTo:
    -residence
  hasMany:
    -purchases

Tally
  belongsTo:
    -user
    -chore

Purchase
  belongsTo:
    -user
    -expense
